{
  "name": "mpp-xml-converter",
  "version": "1.0.0",
  "description": "Conversor web de arquivos .mpp para .xml com sistema de monetização via PIX",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "start:simple": "node api/server-simple.js",
    "test:server": "node api/server-simple.js",
    "worker": "node queue/worker.js",
    "dev": "nodemon api/server.js",
    "dev:simple": "nodemon api/server-simple.js",
    "dev:worker": "nodemon queue/worker.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "syntax-check": "node scripts/syntax-check.js",
    "final-check": "node scripts/final-check.js",
    "setup-dirs": "powershell -Command \"New-Item -ItemType Directory -Force uploads\\incoming, uploads\\processing, uploads\\converted, uploads\\expired, uploads\\quarantine, logs, temp, backups\"",
    "cleanup": "node scripts/cleanup.js",
    "doctor": "npm run syntax-check && npm run lint",
    "build": "npm ci --only=production",
    "postinstall": "npm run setup-dirs"
  },
  "keywords": [
    "mpp",
    "xml",
    "converter",
    "microsoft project",
    "pix",
    "payment",
    "monetization"
  ],
  "author": "Seu Nome <seu.email@exemplo.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "bullmq": "^5.63.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "file-type": "^21.1.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "qrcode": "^1.5.4",
    "redis": "^5.9.0",
    "sanitize-filename": "^1.6.3",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "validator": "^13.15.23",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/mpp-xml-converter.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/mpp-xml-converter/issues"
  },
  "homepage": "https://github.com/seu-usuario/mpp-xml-converter#readme"
}
